{
    "time_units": "days since 1949-12-01 00:00:00",
    "reference_time": "1949-12-01 00:00:00",
    "dask": {
        "enable": true,
        "cluster": {
            "n_workers": 1
        }
    },
    "filename_filtering": {
        "variable": true,
        "year": true
    },
    "copy_coordinates_from_inputs": true,
    "detect_output_frequency_from_inputs": true,
    "schema": "cordex-cmip6.json",
    "historical_cutoff": 2006,
    "metadata_defaults": {
        "ensemble": "%(ensemble)s",
        "rcm_version_id": "%(rcm_version_id)s",
        "rcm_version": "%(rcm_version)s",
        "rcm_version_cordex": "%(rcm_version_cordex)s",
        "rcm_institute": "CSIRO",
        "rcm_model": "%(rcm_model)s",
        "rcm_model_cordex": "%(rcm_model_cordex)s",
        "contact": "%(contact)s",
        "institute_id": "CSIRO",
        "institution": "Commonwealth Scientific and Industrial Research Organisation",
        "references": "https://confluence.csiro.au/display/CCAM",
        "model_id": "%(rcm_institute)s-%(rcm_model)s",
        "experiment": "%(description)s",
        "experiment_id": "%(experiment)s",
        "driving_model_id": "%(gcm_institute)s-%(gcm_model)s",
        "driving_model_ensemble_member": "%(ensemble)s",
        "driving_experiment_name": "%(experiment)s",
        "driving_experiment": "%(gcm_institute)s-%(gcm_model)s; %(experiment)s; %(ensemble)s",
        "project_id": "%(project)s",
        "CORDEX_domain": "%(domain)s",
        "product": "output",
        "frequency": "%(frequency_mapping)s",
        "creation_date": "%(created)s",
        "Conventions": "CF-1.7",
        "tracking_id": "%(uuid)s"
    },
    "frequency_mapping":{
        "12min": "12min",
        "3H": "3hr",
        "1D": "day",
        "1M": "mon",
        "1H": "1hr",
        "6H": "6hr"
    },
    "encoding": {
        "variables": {
            "dtype": "float32",
            "_FillValue": 1e20,
            "zlib": true,
            "complevel": 1
        },
        "lon": {
            "_FillValue": 1e20,
            "dtype": "double"
        },
        "lev": {
            "_FillValue": 1e20,
            "dtype": "double"
        },
        "lat": {
            "_FillValue": 1e20,
            "dtype": "double"
        },
        "depth": {
            "_FillValue": 1e20,
            "dtype": "double"
        },
        "time": {
            "_FillValue": 1e20,
            "dtype": "double",
            "units": "days since 1949-12-01 00:00:00"
        }
    },
    "templates": {
        "drs_base": "%(rcm_institute)s/%(gcm_institute)s-%(gcm_model)s/%(experiment)s/%(ensemble)s/%(rcm_institute)s-%(rcm_model)s/%(rcm_version_id)s",
        "drs_path": "%(output_directory)s/%(project)s/output/%(domain)s/%(rcm_institute)s/%(gcm_institute)s-%(gcm_model)s/%(experiment)s/%(ensemble)s/%(rcm_institute)s-%(rcm_model)s/%(rcm_version_id)s/%(frequency_mapping)s/%(variable)s",
        "filename": "%(variable)s_%(domain)s_%(gcm_institute)s-%(gcm_model)s_%(experiment)s_%(ensemble)s_%(rcm_institute)s-%(rcm_model)s_%(rcm_version_id)s_%(frequency_mapping)s_%(start_date)s-%(end_date)s.nc",
        "filename_fixed": "%(variable)s_%(domain)s_%(gcm_institute)s-%(gcm_model)s_%(experiment)s_%(ensemble)s_%(rcm_institute)s-%(rcm_model)s_%(rcm_version_id)s_%(path_freq)s.nc"
    }
}